# Environment example for Backend Studio
# Copy to .env or set VITE_ variables in your environment.

# Feature toggles
VITE_ENABLE_SW=true
VITE_ENABLE_EMAIL=true
VITE_ENABLE_OPENROUTER=false

# OpenRouter key (only if VITE_ENABLE_OPENROUTER=true)
VITE_OPENROUTER_API_KEY=

# Logging level: debug | info | warn | error | silent
VITE_LOG_LEVEL=info

# Socket server port (development)
VITE_SOCKET_PORT=9150

# Playwright / E2E related
VITE_E2E_BASE_URL=http://localhost:5173
